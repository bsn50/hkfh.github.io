              "instructor_notes": null
            },
            {
              "id": 214406,
              "key": "eed7f696-cb36-48b5-9dfc-77a7642a392d",
              "title": "折线图",
              "semantic_type": "ImageAtom",
              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2016/October/57f3d276_line-plot-color-encoded/line-plot-color-encoded.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/eed7f696-cb36-48b5-9dfc-77a7642a392d",
